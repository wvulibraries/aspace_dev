<%
   section_id = "agent_other_agency_codes" if section_id.blank?
   section_title = "Other Agency Codes" if section_title.blank?
%>

<section id="<%= section_id %>">
  <h3><%= section_title %></h3>
  <div class="details my-3" id="<%= section_id %>_accordion">
    <% agent_other_agency_codes.each_with_index do | agent_other_agency_code, index | %>
      <div class="card mb-3">
        <div class="card-header panel-heading">
          <div class="row accordion-toggle collapsed" data-toggle="collapse" data-parent="#<%= section_id %>_accordion" href="#<%= section_id %>_agent_other_agency_code_<%= index %>">
            <div class="col-md-1">
              <span class="glyphicon"></span>
            </div>
            <div class="col-md-6">
              <%= agent_other_agency_code["maintenance_agency"] %>
            </div>
          </div>
        </div>
        <div id="<%= section_id %>_agent_other_agency_code_<%= index %>" class="panel-collapse collapse">
          <%= read_only_view(agent_other_agency_code) %>
        </div>
      </div>
    <% end %>
  </div>
</section>
